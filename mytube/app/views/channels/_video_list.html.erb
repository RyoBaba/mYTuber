<div class="row">
  <div class="col-md-12">
    <%= paginate videos %>
  </div>
  <% videos.each do |video| %>
    <div class="col-xs-6 col-lg-4">
      <div class="thumbnail">
        <%= render partial: "shared/video_frame", locals: {video: video, width: 258, height: 146} %>
      </div>
    </div>
  <% end %>
  <div class="col-md-12">
    <%= paginate videos %>
  </div>
</div>