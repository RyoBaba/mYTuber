<div class="row">
  <div class="col-md-9">
    <div class="row">
      <div class="col-md-12">
        <%= paginate videos %>
      </div>
      <% videos.each do |video| %>
        <div class="col-xs-6 col-lg-4">
          <div class="thumbnail">
            <%= link_to image_tag(video.thumb_url, width: "256"), 
                video_url(video.youtube_video_id), 
                { target: "_blank" } %>
          </div>
        </div>
      <% end %>
      <div class="col-md-12">
        <%= paginate videos %>
      </div>
    </div>
  </div>
  <div class="col-md-3">
    <%= render partial: "sidebar", locals: {channel: channel, yyyymm: yyyymm} %>
  </div>
</div>